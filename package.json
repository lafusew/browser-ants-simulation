{
  "name": "parcel-typescript",
  "version": "0.0.1",
  "description": "Parcel Typescript \"starter kit\"",
  "main": "src/index.ts",
  "author": "lafusew",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "start": "parcel serve src/index.html",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url .",
