    "deploy": "gh-pages -d dist",
    "lint": "eslint src/index.ts"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "parcel": "^2.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {}
}
